{
    "name":  "@ekairos/claude-reactor",
    "version":  "1.21.83-beta.0",
    "description":  "Claude reactor scaffolding for @ekairos/thread",
    "type":  "module",
    "main":  "./dist/index.js",
    "types":  "./dist/index.d.ts",
    "files":  [
                  "dist"
              ],
    "exports":  {
                    ".":  {
                              "types":  "./dist/index.d.ts",
                              "import":  "./dist/index.js",
                              "default":  "./dist/index.js"
                          }
                },
    "scripts":  {
                    "build":  "tsc -p tsconfig.json",
                    "dev":  "tsc -p tsconfig.json --watch",
                    "clean":  "node -e \"require(\u0027fs\u0027).rmSync(\u0027dist\u0027, {recursive:true, force:true})\"",
                    "typecheck":  "tsc --noEmit"
                },
    "peerDependencies":  {
                             "@ekairos/thread":  "*"
                         },
    "devDependencies":  {
                            "@ekairos/thread":  "workspace:*",
                            "@ekairos/tsconfig":  "workspace:*",
                            "@types/node":  "^24.5.0",
                            "typescript":  "^5.9.2"
                        },
    "repository":  {
                       "type":  "git",
                       "url":  "git+https://github.com/e-kairos/ekairos.git"
                   },
    "homepage":  "https://github.com/e-kairos/ekairos#readme",
    "bugs":  {
                 "url":  "https://github.com/e-kairos/ekairos/issues"
             }
}
